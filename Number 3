#include <iostream>
#include <string>
using namespace std;

int main() {
    string word, reversed = "";

    cout << "Enter a word: ";
    cin >> word;

    for (int i = word.length() - 1; i >= 0; i--) {
        reversed += word[i];
    }

    if (word == reversed) {
        cout << word << " is a palindrome";
    } else {
        cout << word << "Not a Palindrome";
    }    
    return 0;
}
        

